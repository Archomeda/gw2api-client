{
  "name": "gw2e-gw2api-client",
  "version": "2.0.1",
  "description": "A javascript wrapper around the official Guild Wars 2 API",
  "main": "./build/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src/ -d build/",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary --report-dir=coverage/ mocha --compilers js:babel-core/register --require babel-polyfill tests/* && snazzy"
  },
  "author": "queicherius@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gw2efficiency/gw2api-client"
  },
  "nyc": {
    "include": [
      "src/"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "gw2e-requester": "^1.0.0",
    "url-parse": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "isparta": "^4.0.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "rewire": "^2.5.1",
    "snazzy": "^4.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
